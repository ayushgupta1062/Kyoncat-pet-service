{
  "version": 2,
  "rewrites": [
    {
      "source": "/static/(.*)",
      "destination": "/static/$1"
    },
    {
      "source": "/media/(.*)",
      "destination": "/media/$1"
    },
    {
      "source": "/(.*)",
      "destination": "kyonkat/wsgi.py"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "kyonkat.settings"
  }
}